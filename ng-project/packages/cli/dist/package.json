{
  "name": "@ngdev/cli",
  "version": "25.03.255",
  "description": "cli tool",
  "bin": {
    "ngdev": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json --watch",
    "prebuild": "rimraf dist/bin dist/lib dist/src",
