    "@yarnpkg/lockfile": "^1.1.0",
    "ansis": "^3.17.0",
    "chokidar": "^4.0.3",
    "cli-table3": "^0.6.5",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^11.0.1",
