{
    "compilerOptions": {
      "baseUrl": "tsconfig",
      "strict": false,
      // "outDir": "../../dist/cli",
      "lib": ["es2018", "dom"],
      "module": "commonjs",
      "moduleResolution": "node",
      "noEmitOnError": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noUnusedParameters": false,
      "resolveJsonModule": true,
      "noUnusedLocals": false,
      "rootDir": ".",
      "skipDefaultLibCheck": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "strictNullChecks": true,
      "allowJs": true,
      "target": "es6",
      "types": ["jasmine", "node", "yargs"]
    },
    "exclude": ["node_modules", "../../__src__"]
  }