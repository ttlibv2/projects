{
  "name": "schematics-debugged",
  "version": "0.0.0",
  "description": "A schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json --watch",
    "postbuild": "copyfiles ./src/*/schema.json ./src/*/files/**  ./src/collection.json ./package.json dist --watch",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^19.0.4",
    "@angular-devkit/schematics": "^19.0.4",
    "@schematics/angular": "^19.2.0",
    "typescript": "~5.6.2"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "copyfiles": "^2.4.1",
    "jasmine": "~5.4.0"
  }
}
